{
  "id": "story-arcs",
  "displayName": "Story Arcs",
  "resolverType": "titleAppearanceOrder",
  "contentType": "groups",
  "titleFilter": "\\[.+\\]",
  "titleExtractor": {
    "source": "title",
    "pattern": "\\[(.+?)\\s+\\d+\\]",
    "group": 1
  },
  "smartPlaylistEpisodeExtractor": {
    "source": "title",
    "pattern": "\\[.+?\\s+(\\d+)\\]",
    "seasonGroup": null,
    "episodeGroup": 1,
    "fallbackToRss": true
  }
}
